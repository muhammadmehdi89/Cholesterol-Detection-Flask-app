{
    "version": 2,
    "builds": [
        {"src": "app.py", "use": "@vercel/python"}
    ],
    "routes": [
        {"src": "/static/(.*)", "dest": "/static/$1"},
        {"src": "/contact", "dest": "/contact.html"},
        {"src": "/process_form", "dest": "/process_form.html"},
        {"src": "/(.*)", "dest": "app.py"}
    ]
}
